keycode 166 = F19
keycode 167 = F20
keycode 77 = Num_Lock
! No Caps Lock
clear lock
! Caps Lock as Win key
add mod4 = Caps_Lock

remove Lock = Caps_Lock
keysym Caps_Lock = Super_L

! AltGr + l -> slash
keycode 46 = l L l L slash slash


